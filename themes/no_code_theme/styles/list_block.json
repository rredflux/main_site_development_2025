{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "ListBlockStyle",
	"slug": "ListBlockStyle",
	"blockTypes": [ "core/list" ],
	"styles": {
		"border": {
            "style": "solid",
			"radius": "1rem",
            "color": "#FFFFFF",
            "width": "0.25rem"
		},
		"color": {
			"text": "#000000"
		},
        "typography": {
            "fontFamily": "AnonymousPro",
            "fontSize": "2vw",
            "lineHeight": "1.7"
	    },
        "spacing": {
            "padding": {
                "top": "1.25vw",
                "bottom": "1.25vw",
                "left": "3vw",
                "right": "3vw"
            }
        }
    }   
}