{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"defaultDuotone": false,
			"custom": false,
			"customGradient": false,
			"customDuotone": false,
			"palette": [
				{
					"slug": "base",
					"color": "#E1E1E1",
					"name": "Base"
				},
				{
					"slug": "contrast",
					"color": "#000000",
					"name": "Contrast"
				},
				{
					"slug": "primary",
					"color": "#E1E1E1",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#FFFFFF",
					"name": "Secondary"
				},
				{
					"slug": "primary_gradient",
					"color": "#5E2D92",
					"name": "primary_gradient"
				},
				{
					"slug": "secondary_gradient",
					"color": "#F41D77",
					"name": "secondary_gradient"
				}
			]
		},
		"layout": {
			"contentSize": "1000px",
			"wideSize": "1440px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"fontFamilies": [
				{
					"name": "System (Roboto, Segoe UI, Ubuntu)",
					"slug": "system",
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				},
				{
					"name": "OxygenMono",
					"slug": "OxygenMono",
					"fontFamily": "OxygenMono",
					"fontFace": [
						{
							"fontFamily": "OxygenMono",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": ["file:./assets/fonts/oxygen_mono_regular.ttf"]
						}
					]
				},
				{
					"name": "AnonymousPro",
					"slug": "AnonymousPro",
					"fontFamily": "AnonymousPro",
					"fontFace": [
						{
							"fontFamily": "AnonymousPro",
							"fontWeight": "700",
							"fontStyle": "bold",
							"fontStretch": "normal",
							"src": ["file:./assets/fonts/AnonymousPro_Bold.ttf"]
						},
						{
							"fontFamily": "AnonymousPro",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": ["file:./assets/fonts/AnonymousPro_Regular.ttf"]
						},
						{
							"fontFamily": "AnonymousPro",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": ["file:./assets/fonts/AnonymousPro_Italic.ttf"]
						}
					]
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system)"
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		}
	}
}
